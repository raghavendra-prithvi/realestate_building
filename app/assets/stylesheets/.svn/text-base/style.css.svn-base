body{font:normal 12px Arial, Helvetica, sans-serif;}
.header .logo{width:218px; height:71px; padding-left:23px; float:left; }
.header ul{float:right; margin-top:30px; padding-right:30px;}
.header ul li{padding:0 0 0 20px; display:inline;}
.header ul li a, .header ul li a:visited{color:#fff; text-decoration:none;}
.header ul li a:hover{color:#cecece;}
.header ul li img{margin:0px 5px 0 0;}
.header .nav ul{float:right; margin-top:0; padding-right:10px;}
.header .nav ul li{float:left; padding:0; margin-right:2px;}
.header .nav ul li a, .header .nav ul li a:visited{text-transform:uppercase; color:#fff; text-decoration:none; line-height:40px; padding:0 25px; display:block;}
.header .nav ul li a:hover, .header .nav ul li.active{background:#3b9cd3;}
.content .top-section{width:980px; overflow:hidden; height:380px;}
.content .top-section .left-block{width:400px; position:relative; background:url(/assets/shade_box_c.jpg) left top repeat-x #fff;  height:341px; margin-right:10px; float:left;}
.content .top-section .left-block .cont-heading{width:380px; float:left;}
.content .top-section .left-block .cont-heading h3{font:normal 26px Arial, Helvetica, sans-serif; color:#1c78ac; padding:25px 0 20px 25px;}
.content .top-section .left-block .cont-heading p{font:normal 14px Arial, Helvetica, sans-serif; color:#6b6b6b; padding:0px 0 20px 25px; line-height:20px;}
.content .top-section .right-block{width:570px; position:relative; background:url(/assets/shade_box_c.jpg) left top repeat-x #fff;  height:341px;  float:left; display:inline-block;}
.left-bg{background:url(/assets/shade_box_l.jpg) left top no-repeat; left:0; top:0px; width:10px; height:341px; float:left;}
.right-bg{background:url(/assets/shade_box_r.jpg) left top no-repeat; right:0; top:0px; width:10px; height:341px; float:right;}
.right-no-bg{right:0; top:0px; width:10px; height:341px; float:right;}
.content .top-section .right-block .theme-img{float:left; margin: 30px 0 0 35px;}
.content .btom-section{width:980px; overflow:hidden; height:203px;}
.content .btom-section .gray-bg-left{width:10px; float:left; background:url(/assets/gray_bg_left.jpg) left top no-repeat; height:203px;}
.content .btom-section .gray-bg-right{width:10px; float:left; background:url(/assets/gray_bg_right.jpg) left top no-repeat; height:203px;}
.content .btom-section .gray-bg-center{float:left; background:url(/assets/gray_bg_center.jpg) left top repeat-x; width:960px; height:203px;}
.content .btom-section .gray-bg-center .left-section{display:block; width:311px; height:129px; float:left; margin:38px 38px 0 28px; background:url(/assets/post_listing.jpg) left top no-repeat; cursor:pointer;}
.content .btom-section .gray-bg-center .left-section:hover{display:block; background:url(/assets/post_listing_h.jpg) left top no-repeat;}
.content .btom-section .gray-bg-center .center-section{width:276px; height:129px; float:left; margin:38px 0 0 0;}
.content .btom-section .gray-bg-center .right-section{width:266px; height:111px; float:left; margin:38px 0 0 0; background:url(/assets/home_form_bg.jpg) left top no-repeat; padding:18px 0 0 10px;}
.content .btom-section .gray-bg-center .right-section .form-bg{width:227px; height:75px; background:url(/assets/form_bg.jpg) left top no-repeat; padding:20px 0 0 20px;}
.content .btom-section .gray-bg-center .right-section .form-bg .radio-text{color:#1c78ac; font:bold 16px Arial, Helvetica, sans-serif; padding-right:10px;}
.pad-right{padding-right:15px!important;}
.content .btom-section .gray-bg-center .right-section .form-bg .text-box{width:140px; height:22px; background:#f5f5f5; line-height:22px; padding:0 2px; border:1px solid #d1d1d1; margin-bottom:10px; font:normal 12px Arial, Helvetica, sans-serif; color:#a2a2a2;}

